#/bin/bash
SHELL_PROJECT_PATH=/home/bdg/ssd/shell/translation_sreader_sdk_newcore
APK_PATH=BrowserActivity/build/outputs/apk/BrowserActivity-debug.apk
DEST_APK_PATH=/var/www/
